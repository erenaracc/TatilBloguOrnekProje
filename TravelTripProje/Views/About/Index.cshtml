@using TravelTripProje.Models.Sınıflar
@model List<Hakkımızda>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<body>


    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>Hakkımızda</h3>

                @foreach (var x in Model)
                {
                    <img src="@x.FotoUrl" width="500" height="250" alt="" />
                    <p>@x.Aciklama </p>
                }

            </div>
        </div>
    </div>

</body>


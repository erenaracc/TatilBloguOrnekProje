@using TravelTripProje.Models.Sınıflar
@model List<Blog>


<div class="left-text">
    <div class="padding">
        <h4>Gezdiğim 10 Yer</h4>

        <ul class="list list-pad">
            @{ int i = 0;}
            @foreach (var x in Model)
            {
                <li class="list-bg"><span>@{ i++;}@i.</span><a href="/Blog/BlogDetay/@x.Id">@x.Baslik</a></li>
            }
        </ul>
        <a href="/Blog/Index" class="link1">Tüm Blogları Göre</a>
    </div>
    </div>
    
